<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>DisplayServlet</servlet-name>
    <display-name>DisplayServlet</display-name>
    <description></description>
    <servlet-class>com.klef.ep.services.DisplayServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InsertEmpServlet</servlet-name>
    <display-name>InsertEmpServlet</display-name>
    <description></description>
    <servlet-class>com.klef.ep.services.InsertEmpServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckEmpLoginServlet</servlet-name>
    <display-name>CheckEmpLoginServlet</display-name>
    <description></description>
    <servlet-class>com.klef.ep.services.CheckEmpLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayServlet</servlet-name>
    <url-pattern>/display</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertEmpServlet</servlet-name>
    <url-pattern>/insertemp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckEmpLoginServlet</servlet-name>
    <url-pattern>/checkemp</url-pattern>
  </servlet-mapping>
</web-app>